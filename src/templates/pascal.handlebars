var
    N, S, D, J, i, grader: integer;
    header: string;
    A, Ds, Js: array [1..10] of integer;

begin
    readln(header);

    {{#ifCond data.length '==' 0}}
    // Tulis kode Anda di bagian ini untuk subtask-subtask lainnya.
    {{else}}
    {{#each data}}
    {{#ifCond data.length '>' 0}}else {{/ifCond}}if (header[{{math testcase '+' 1}}] = '{{testcase}}') then begin
        readln(N, S); // N = {{N}}, S = {{S}}
